{
  "env": [
    {
      "name": ["NChain-v1"],
      "cfg": [{"n": [3]}],
      "max_episode_steps": [1],
      "input_type": ["feature"]
    }
  ],
  "agent": [
    {
      "name": ["DQN", "DDQN"]
    },
    {
      "name": ["MaxminDQN", "AveragedDQN"],
      "target_networks_num": [4]
    }
  ],
  "train_steps": [5e3],
  "test_per_episodes": [10],
  "device": ["cuda:0"],
  "hidden_layers": [[64,64]],
  "memory_type": ["FiniteReplay"],
  "memory_size": [1e4],
  "exploration_type": ["LinearEpsilonGreedy"],
  "exploration_steps": [1e3],
  "epsilon_steps": [1e3],
  "epsilon_start": [0.1],
  "epsilon_end": [0.1],
  "epsilon_decay": [1],
  "loss": ["MSELoss"],
  "optimizer": [
    {
      "name": ["RMSprop"],
      "kwargs": [{"lr": [3e-4]}]
    }
  ],
  "batch_size": [32],
  "display_interval": [100],
  "rolling_score_window": [{"Train": [20], "Test": [5]}],
  "discount": [0.99],
  "seed": [1],
  "show_tb": [false],
  "gradient_clip": [-1],
  "target_network_update_steps": [100],
  "network_update_steps": [1],
  "generate_random_seed": [true]
}