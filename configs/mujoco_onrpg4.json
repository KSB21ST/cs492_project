{
  "env": [
    {
      "name": ["HalfCheetah-v2"],
      "max_episode_steps": [-1],
      "input_type": ["feature"]
    }
  ],
  "train_steps": [3e6],
  "steps_per_epoch": [2048],
  "test_per_epochs": [-1],
  "agent": [{"name": ["OnRPG4"]}],
  "optimizer": [
    {
      "name": ["Adam"],
      "actor_kwargs": [{"lr": [3e-4]}],
      "critic_kwargs": [{"lr": [1e-3]}]
    }
  ],
  "batch_size": [64],
  "optimize_epochs": [1, 3, 10, 30],
  "gradient_clip": [-1],
  "hidden_layers": [[64,64]],
  "hidden_act": ["Tanh"],
  "display_interval": [20],
  "rolling_score_window": [{"Train": [20], "Test": [10]}],
  "discount": [0.99],
  "seed": [1],
  "device": ["cpu"],
  "show_tb": [false],
  "generate_random_seed": [true]
}