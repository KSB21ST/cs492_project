{
  "env": [
    {
      "name": ["HalfCheetah-v2", "Hopper-v2", "Ant-v2"],
      "max_episode_steps": [-1],
      "input_type": ["feature"]
    }
  ],
  "train_steps": [3e6],
  "steps_per_epoch": [2048],
  "test_per_epochs": [-1],
  "agent": [{"name": ["OnRPG"]}],
  "optimizer": [
    {
      "name": ["Adam"],
      "actor_kwargs": [{"lr": [3e-3, 1e-3, 3e-4, 1e-4, 3e-5]}],
      "critic_kwargs": [{"lr": [1e-3]}]
    }
  ],
  "adv_div_std": [true, false],
  "clip_obj": [true, false],
  "state_normalizer": [true, false],
  "actor_select": ["MLPGaussianActor", "MLPStdGaussianActor", "MLPSquashedGaussianActor"],
  "batch_size": [64],
  "clip_ratio": [0.2],
  "target_kl": [0.01],
  "optimize_epochs": [10],
  "gradient_clip": [-1],
  "hidden_layers": [[64,64]],
  "hidden_act": ["Tanh"],
  "display_interval": [20],
  "rolling_score_window": [{"Train": [20], "Test": [10]}],
  "discount": [0.99],
  "seed": [99],
  "device": ["cpu"],
  "show_tb": [false],
  "generate_random_seed": [false]
}